{
  "name": "population-management-system",
  "version": "1.0.0",
  "description": "A population management system",
  "main": "server.js",
  "repository": "git@github.com:efalayi/population-management-system.git",
  "author": "Esther Falayi <esther.falayi@andela.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all build:config build:constants build:db build:server build:client",
    "build:client": "./node_modules/.bin/webpack --config webpack.prod.config.babel.js",
    "build:config": "babel --root-mode upward config -d dist/config",
    "build:constants": "babel --root-mode upward constants -d dist/constants",
    "build:db": "babel --root-mode upward database/models -d dist/database/models",
    "build:server": "babel --root-mode upward server -d dist/server",
    "build:dev:server": "babel-node server/server.js",
    "clean:dist": "rimraf dist && mkdir dist",
    "client:coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "nyc report --reporter=text-lcov | ./node_modules/coveralls/bin/coveralls.js",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:reset": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "lint": "./node_modules/.bin/eslint './**/*.js'",
    "postinstall": "yarn run db:migrate",
    "prebuild": "yarn run clean:dist",
    "prestart": "yarn run build",
    "start": "node dist/server/server.js",
    "start:api:dev": "nodemon",
    "start:dev": "npm-run-all --parallel start:api:dev start:react:dev",
    "start:react:dev": "webpack-dev-server --config webpack.dev.config.babel.js --watch",
    "test": "npm-run-all db:reset db:migrate test:server",
    "test:client": "./node_modules/.bin/jest --ci --config jest.config.js --coverage",
    "test:server": "./node_modules/.bin/nyc ./node_modules/.bin/mocha --opts ./mocha.opts"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.8.1",
    "@material-ui/icons": "^3.0.1",
    "babel-core": "^7.0.0-bridge",
    "babel-loader": "^8.0.4",
    "body-parser": "^1.18.3",
    "chalk": "2.4.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "pushid": "^1.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.1.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "rosie": "^2.0.1",
    "supertest": "^3.3.0",
    "webpack-dev-server": "^3.1.14"
  }
}
